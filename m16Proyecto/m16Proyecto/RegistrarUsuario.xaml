<Window x:Class="m16Proyecto.RegistrarUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:m16Proyecto"
        mc:Ignorable="d"
        Title="RegistrarUsuario" Height="300" Width="425.333">
    <Grid>
        <Button x:Name="button" Content="Salir" HorizontalAlignment="Left" Margin="173,219,0,0" VerticalAlignment="Top" Width="75" Click="botonSalir_Click"/>
        <Label x:Name="label1" Content="Nombre" HorizontalAlignment="Left" Margin="20,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Primer Apellido" HorizontalAlignment="Left" Margin="20,60,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label3" Content="Segundo Apellido" HorizontalAlignment="Left" Margin="204,60,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4" Content="DNI" HorizontalAlignment="Left" Margin="20,7,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.495,0.513"/>
        <Label x:Name="label5" Content="Correo" HorizontalAlignment="Left" Margin="20,86,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label6" Content="Telefono" HorizontalAlignment="Left" Margin="20,112,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Direccion" HorizontalAlignment="Left" Margin="204,86,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label7" Content="Población" HorizontalAlignment="Left" Margin="204,113,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label8" Content="Usuario" HorizontalAlignment="Left" Margin="20,139,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label9" Content="Contraseña" HorizontalAlignment="Left" Margin="204,140,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label10" Content="Categoria" HorizontalAlignment="Left" Margin="20,166,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
