<Window x:Class="m16Proyecto.GestionarUsuarios"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:m16Proyecto"
        mc:Ignorable="d"
        Title="GestionarUsuarios" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="Agregar" HorizontalAlignment="Left" Margin="22,23,0,0" VerticalAlignment="Top" Width="108" Height="45" FontSize="14" FontFamily="Segoe UI Black" Click="botonAgregar_Click"/>
        <Button x:Name="button1" Content="Eliminar" HorizontalAlignment="Left" Margin="22,74,0,0" VerticalAlignment="Top" Width="108" Height="45" FontSize="14" FontFamily="Segoe UI Black" Click="botonEliminar_Click"/>
        <Button x:Name="button2" Content="Consultar" HorizontalAlignment="Left" Margin="22,124,0,0" VerticalAlignment="Top" Width="108" Height="45" FontSize="14" FontFamily="Segoe UI Black" Click="botonConsultar_Click"/>
        <Button x:Name="button3" Content="Volver" HorizontalAlignment="Left" Margin="198,229,0,0" VerticalAlignment="Top" Width="75" Click="botonVolver_Click"/>

    </Grid>
</Window>
